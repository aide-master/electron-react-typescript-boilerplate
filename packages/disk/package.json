{
  "name": "@aidemaster/disk",
  "version": "1.0.0",
  "description": "Disk Utility",
  "main": "main.js",
  "scripts": {
    "start": "BROWSER=none NODE_ENV=development PORT=3000 concurrently \"npm run start:react\" \"wait-on http://localhost:3000/ && npm run start:electron\"",
    "start:electron": "electron .",
    "start:react": "react-scripts start",
    "build": "npm run build:react && npm run build:electron",
    "build:electron": "electron-packager .",
    "build:react": "react-scripts build",
    "test": "react-scripts test"
  },
  "repository": "https://github.com/aide-master/disk",
  "homepage": ".",
  "keywords": [
    "Electron",
    "Disk",
    "Utility"
  ],
  "author": "Yudu Ban<banyudu@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "electron": "^13.6.6"
  },
  "dependencies": {
    "@types/jest": "24.0.16",
    "@types/node": "12.6.8",
    "@types/react": "16.8.23",
    "@types/react-dom": "16.8.5",
    "concurrently": "^4.1.1",
    "npm-run-all": "^4.1.5",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1",
    "ts-node": "^8.3.0",
    "typescript": "3.5.3",
    "wait-on": "^3.3.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
